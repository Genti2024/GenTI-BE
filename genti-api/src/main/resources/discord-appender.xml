<included>
    <appender name="DISCORD" class="com.gt.genti.discord.DiscordAppender">
        <errorChannelUrl>${BASE_URL}${ERROR_PATH}</errorChannelUrl>
        <adminChannelUrl>${BASE_URL}${ADMIN_PATH}</adminChannelUrl>
        <eventChannelUrl>${BASE_URL}${EVENT_PATH}</eventChannelUrl>
    </appender>
    <appender name="ASYNC_DISCORD" class="ch.qos.logback.classic.AsyncAppender">
        <appender-ref ref="DISCORD"/>
        <filter class="ch.qos.logback.classic.filter.ThresholdFilter">
            <level>ERROR</level>
        </filter>
    </appender>
</included>
